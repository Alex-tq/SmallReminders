{
  "name": "smallreminders",
  "version": "1.0.0",
  "description": "node.js backend to keep track and send messages",
  "private": true,
  "scripts": {
    "test": "semistandard && NODE_ENV=TEST jest --detectOpenHandles",
    "start": "npm run start-server & npm run start-client ",
    "start-server": "NODE_ENV=DEV nodemon server/index.js",
    "start-client": "webpack-dev-server --config webpack.dev.js --open --hot",
    "start-prod": "node server/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elenamik/SmallReminders.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/elenamik/SmallReminders/issues"
  },
  "homepage": "https://github.com/elenamik/SmallReminders#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongodb": "^3.4.0",
    "mongoose": "^5.8.1",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-merge": "^4.2.2"
  },
  "devDependencies": {
    "semistandard": "^14.2.0",
    "jest": "^24.9.0",
    "nodemon": "^2.0.2",
    "supertest": "^4.0.2",
    "webpack-dev-server": "^3.10.1"
  },
  "semistandard": {
    "env": {
      "jest": true
    }
  }
